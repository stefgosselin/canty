{
    "name": "madfrog/canty-cli",
    "description": "cc (canty cli) is a minimalist scaffold used for quick and (not so dirty) cli tools.",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Madfrog\\CantyCli\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Stephane Gosselin",
            "email": "stephane.gosselin@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "symfony/console": "^6.0",
        "symfony/config": "^6.0",
        "symfony/dependency-injection": "^6.0",
        "symfony/yaml": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src"
        }
    },
    "require-dev": {
        "symfony/var-dumper": "^6.0"
    }
}
